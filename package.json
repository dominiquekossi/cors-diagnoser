{
  "name": "cors-diagnoser",
  "version": "1.0.0",
  "description": "Automatic CORS error diagnostics for Express backend and browser frontend. Detects 10+ common patterns, provides clear explanations, code examples, and security recommendations to fix Cross-Origin Resource Sharing issues quickly.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "cors",
    "cors-error",
    "cors-debugging",
    "cors-diagnoser",
    "cors-analyzer",
    "cors-middleware",
    "cors-helper",
    "cross-origin",
    "cross-origin-resource-sharing",
    "express",
    "express-middleware",
    "middleware",
    "debugging",
    "diagnostics",
    "http-headers",
    "preflight",
    "access-control",
    "browser",
    "frontend",
    "backend",
    "api",
    "rest-api",
    "security",
    "web-development",
    "developer-tools",
    "error-handling",
    "typescript"
  ],
  "author": "Dominique Kossi <houessoudominique@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dominiquekossi/cors-diagnoser.git"
  },
  "bugs": {
    "url": "https://github.com/dominiquekossi/cors-diagnoser/issues"
  },
  "homepage": "https://github.com/dominiquekossi/cors-diagnoser#readme",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "express": "^5.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  }
}
